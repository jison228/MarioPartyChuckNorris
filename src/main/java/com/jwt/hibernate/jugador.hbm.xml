<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="com.jwt.hibernate.Jugador" table="JUGADOR">
		<id column="Username" name="username" type="string" />
		<property column="password" name="password" type="string"></property>
		<property column="wins" name="wins" type="int"></property>
		<property column="maxminigame" name="maxminigame" type="int"></property>
	</class>
</hibernate-mapping>